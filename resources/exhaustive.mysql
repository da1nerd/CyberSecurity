SELECT pcount.person_id AS 'person_id', count AS 'degree', p.name AS 'name', ppl.contact_id AS 'contact_id' 
FROM 
(
	SELECT person_id, COUNT(contact_id) as 'count' 
	FROM person_person_link 
	GROUP BY person_id
) AS pcount 
INNER JOIN person_person_link as ppl ON ppl.person_id = pcount.person_id 
INNER JOIN person as p ON p.id = pcount.person_id 
WHERE count >= 25 AND count <= 35
